	//	10.	Escribir un programa que detecte si un número leído desde el teclado es mayor o menor que 100.
	definir num Como Entero
	escribir "Ingresa un numero entero "
	leer num
	si num=100 Entonces
		escribir "Es igual a 100"
	Sino
		si num>100 Entonces
			escribir "Es mayor que 100"
		Sino
			escribir "Es menor que 100"
		FinSi
	FinSi
FinProceso
